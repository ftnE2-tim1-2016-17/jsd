Query:
    serviceType = ServiceType
    serviceItem = ServiceItem

    parameters *= Parameters
;

ServiceType:
    'iznajmljivanje'|'kupovina'
;

ServiceItem:
    (car = Car) | (realEstate = RealEstate)
;

Car:
    "automobila" | "auto" | "auta" | "automobila" | "vozila"
;

RealEstate:
    "stana" | "kuca" | "kuce" | "stan"
;

Parameters:
    (city = City) | (dateFrom = DateFrom) | (dateTo = DateTo) | (timeFrom = TimeFrom) | (timeTo = TimeTo) | (itemParameters = ItemParameters) | (realEstateParameters = RealEstateParameters)
;

City:
    'beograd'|'novi sad'|'nis'
;

DateFrom:
    "od" dateFrom = Date
;

TimeFrom:
   "od" "h" timeFrom = Time
;

DateTo:
    "do" dateTo = Date
;

TimeTo:
    "do" "h" timeTo = Time
;

Date:
    day = Day (('.'|'/'|'-') month = Month (('.'|'/'|'-') year = Year)?)?
;

Day:
    number = Number
;

Month:
    number = Number
;

Year:
    number = Number
;

Number:
    /[0-9]+/
;

Time:
    hour = Hour ((':'|'.'|'-') minute = Minute)?
;

Hour:
    number = Number
;

Minute:
    number = Number
;

ItemParameters:
    carParameters *= CarParameters
;

CarParameters:
    (carBrand = CarBrand) | (fuelType = FuelType) | (gearbox = Gearbox) | (carClass = CarClass) | (priceFrom = PriceFrom) | (priceTo = PriceTo)
;

CarBrand:
    "alfa romeo"|"audi"|"bmw"|"chevrolet"|"citroen"|"dacia"|"fiat"|"ford"|"honda"|"hyundai"|"jaguar"|"jeep"|"kia"|"lancia"|
    "mazda"|"mercedes"|"nissan"|"opel"|"peugeot"|"renault"|"seat"|"skoda"|"suzuki"|"volkswagen"|"vw"|"volvo"
;

FuelType:
    "benzin"|"plin"|"dizel"|"eurodizel"|"benzin plin"
;

Gearbox:
    "automatik"|"manuelni"|"poluautomatik"
;

CarClass:
    "gradski"|"niska klasa"|"srednja klasa"|"visoka klasa"|"luksuzni"|"skuter"|"limuzina"|"kombi"
;

PriceFrom:
    "od""e" number = Number
;

PriceTo:
    "do""e" number = Number
;

RealEstateParameters:
    'to be implemented'
;